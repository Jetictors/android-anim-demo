        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">

        <activity
            android:name=".ui.MainActivity"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name=".ui.frame.FrameAnimActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".ui.tween.TweenActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".ui.property.PropertyActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".ui.property.value.ValueAnimatorActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".ui.property.obj.ObjectAnimatorActivity"
            android:screenOrientation="portrait"/>



        <activity
            android:name=".ui.canvas.CanvasActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".ui.other.OtherActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".ui.other.ly.LayoutAnimActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".ui.other.act.TestActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".ui.other.frag.TestFragmentAnimActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".ui.other.motion.MotionActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".ui.other.motion.MotionFirstActivity"
            android:screenOrientation="portrait"/>

    </application>

</manifest>